{
  "bindings": [
      "type": "serviceBusTrigger",
      "name": "message",
      "queueName": "bot_send",
      "connection": "AzureWebJobsServiceBus",
      "accessRights": "manage",
      "direction": "in"
    },
    {
      "type": "bot",
      "name": "$return",
      "direction": "out",
      "botId": "testbot"
    },
    {
      "type": "http",
      "name": "res",
      "direction": "out"
    }
  ]
}