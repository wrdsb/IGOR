{
  "bindings": [
    {
      "type": "serviceBusTrigger",
      "name": "message",
      "queueName": "calendars_to_read",
      "connection": "AzureWebJobsServiceBus",
      "accessRights": "manage",
      "direction": "in"
    },
    {
      "type" : "serviceBus",
      "name" : "$return",
      "topicName" : "calendars_activity",
      "subscriptionName" : "calendar_read",
      "connection" : "AzureWebJobsServiceBus",
      "accessRights" : "manage",
      "direction" : "out"
    },
    {
      "type": "blob",
      "name": "resultBlob",
      "path": "calendars/{calendarId}.json",
      "connection": "wrdsbigor_STORAGE",
      "direction": "out"
    }
  ]
}
