{
  "bindings": [
    {
      "type": "serviceBusTrigger",
      "name": "message",
      "queueName": "calendar_acls_to_list",
      "connection": "AzureWebJobsServiceBus",
      "accessRights": "manage",
      "direction": "in"
    },
    {
      "type": "blob",
      "name": "resultBlob",
      "path": "calendar-acls/{calendarId}.json",
      "connection": "wrdsbigor_STORAGE",
      "direction": "out"
    },
    {
      "type" : "serviceBus",
      "name" : "$return",
      "topicName" : "log_to_slack",
      "subscriptionName" : "calendar_acl_update",
      "connection" : "AzureWebJobsServiceBus",
      "accessRights" : "manage",
      "direction" : "out"
    }
  ]
}