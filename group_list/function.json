{
  "bindings": [
    {
      "type": "serviceBusTrigger",
      "name": "message",
      "queueName": "groups_to_list",
      "connection": "AzureWebJobsServiceBus",
      "accessRights": "manage",
      "direction": "in"
    },
    {
      "type": "blob",
      "name": "groupsAll",
      "path": "groups/groups.all.json",
      "connection": "wrdsbcodex_STORAGE",
      "direction": "out"
    },
    {
      "type": "blob",
      "name": "groupsCreatedAdmin",
      "path": "groups/groups.created.admin.json",
      "connection": "wrdsbcodex_STORAGE",
      "direction": "out"
    },
    {
      "type": "blob",
      "name": "groupsCreatedUser",
      "path": "groups/groups.created.user.json",
      "connection": "wrdsbcodex_STORAGE",
      "direction": "out"
    },
    {
      "type" : "serviceBus",
      "name" : "$return",
      "queueName" : "log_to_slack",
      "connection" : "AzureWebJobsServiceBus",
      "accessRights" : "manage",
      "direction" : "out"
    }
  ]
}
