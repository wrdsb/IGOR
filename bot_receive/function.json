{
  "bindings": [
    {
      "type": "serviceBusTrigger",
      "name": "message",
      "queueName": "bot_receive",
      "connection": "AzureWebJobsServiceBus",
      "accessRights": "manage",
      "direction": "in"
    },
    {
      "type": "serviceBus",
      "name": "$return",
      "queueName": "bot_send",
      "connection": "AzureWebJobsServiceBus",
      "accessRights": "manage",
      "direction": "out"
    }
  ]
}